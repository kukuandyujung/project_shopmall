			<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>			
			<script>
			  const ctx = document.getElementById('myChart');					
			  new Chart(ctx, {
			    type: 'bar',
			    data: {
			      labels: ['3월', '4월', '5월', '6월', '7월', '8월'],
			      datasets: [{
			        label: '월별 매출액',
			        backgroundColor: [
			        	'rgba(255, 0, 0, 0.1)',     // Red, 컬러 세기 0.1
